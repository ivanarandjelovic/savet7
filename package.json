{
  "name": "savet7",
  "version": "0.0.3",
  "description": "Manages some stuff, and demonstrates some test-driven techniques",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivanarandjelovic/savet7.git"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
      "src/main/resources/dev_public/js/actions/appActions.js": true,
      "src/main/resources/dev_public/js/actions/loginActions.js": true,
      "src/main/resources/dev_public/js/actions/buildingActions.js": true
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testDirectoryName": "js",
    "testPathDirs": [
      "src/test/js"
    ],
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.0",
    "counterpart": "^0.17.1",
    "font-awesome": "^4.5.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.3.1",
    "gulp-eslint": "^1.1.1",
    "gulp-open": "^1.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "jasmine-core": "^2.4.1",
    "jasmine-reporters": "^2.1.1",
    "jquery": "^2.2.0",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-matchers": "^2.0.0-beta2",
    "karma-junit-reporter": "^0.3.8",
    "karma-ng-html2js-preprocessor": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.3",
    "phantomjs": "^1.9.19",
    "protractor-jasmine2-html-reporter": "0.0.5",
    "pubsub-js": "^1.5.3",
    "react": "^0.14.7",
    "react-addons-linked-state-mixin": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-modal": "^0.6.1",
    "react-redux": "^4.1.2",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.1.2",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "toastr": "^2.1.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
